{"ast":null,"code":"export default {\n  props: {\n    data: Array,\n    columns: Array,\n    filterKey: String\n  },\n  data() {\n    return {\n      sortKey: '',\n      sortOrders: this.columns.reduce((o, key) => (o[key] = 1, o), {})\n    };\n  },\n  computed: {\n    filteredData() {\n      const sortKey = this.sortKey;\n      const filterKey = this.filterKey && this.filterKey.toLowerCase();\n      const order = this.sortOrders[sortKey] || 1;\n      let data = this.data;\n      if (filterKey) {\n        data = data.filter(row => {\n          return Object.keys(row).some(key => {\n            return String(row[key]).toLowerCase().indexOf(filterKey) > -1;\n          });\n        });\n      }\n      if (sortKey) {\n        data = data.slice().sort((a, b) => {\n          a = a[sortKey];\n          b = b[sortKey];\n          return (a === b ? 0 : a > b ? 1 : -1) * order;\n        });\n      }\n      return data;\n    }\n  },\n  methods: {\n    sortBy(key) {\n      this.sortKey = key;\n      this.sortOrders[key] = this.sortOrders[key] * -1;\n    },\n    capitalize(str) {\n      return str.charAt(0).toUpperCase() + str.slice(1);\n    }\n  }\n};","map":{"version":3,"mappings":"AAsBA,eAAe;EACbA,KAAK,EAAE;IACLC,IAAI,EAAEC,KAAK;IACXC,OAAO,EAAED,KAAK;IACdE,SAAS,EAAEC;EACb,CAAC;EACDJ,IAAI,GAAG;IACL,OAAO;MACLK,OAAO,EAAE,EAAE;MACXC,UAAU,EAAE,IAAI,CAACJ,OAAO,CAACK,MAAM,CAAC,CAAEC,CAAC,EAAEC,GAAG,MAAOD,CAAC,CAACC,GAAG,IAAI,CAAC,EAAGD,CAAC,CAAC,EAAE,CAAC,CAAC;IACpE;EACF,CAAC;EACDE,QAAQ,EAAE;IACRC,YAAY,GAAG;MACb,MAAMN,OAAM,GAAI,IAAI,CAACA,OAAM;MAC3B,MAAMF,SAAQ,GAAI,IAAI,CAACA,SAAQ,IAAK,IAAI,CAACA,SAAS,CAACS,WAAW,EAAC;MAC/D,MAAMC,KAAI,GAAI,IAAI,CAACP,UAAU,CAACD,OAAO,KAAK;MAC1C,IAAIL,IAAG,GAAI,IAAI,CAACA,IAAG;MACnB,IAAIG,SAAS,EAAE;QACbH,IAAG,GAAIA,IAAI,CAACc,MAAM,CAAEC,GAAG,IAAK;UAC1B,OAAOC,MAAM,CAACC,IAAI,CAACF,GAAG,CAAC,CAACG,IAAI,CAAET,GAAG,IAAK;YACpC,OAAOL,MAAM,CAACW,GAAG,CAACN,GAAG,CAAC,CAAC,CAACG,WAAW,EAAE,CAACO,OAAO,CAAChB,SAAS,IAAI,CAAC;UAC9D,CAAC;QACH,CAAC;MACH;MACA,IAAIE,OAAO